          <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="Generar TEST_EMAIL y TEST_PASSWORD" enabled="true">
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="script">// JSR223 (Groovy) - Generar credenciales únicas por hilo
def tnum = ctx.getThreadNum()                       // número del hilo
def host = java.net.InetAddress.getLocalHost().getHostName().replaceAll(&apos;[^A-Za-z0-9]&apos;,&apos;&apos;)
def uuid = java.util.UUID.randomUUID().toString().replaceAll(&apos;-&apos;,&apos;&apos;).substring(0,8)
def stamp = System.currentTimeMillis().toString().takeRight(6)

def email = &quot;user_${host}_${tnum}_${stamp}_${uuid}@example.com&quot;
def password = &quot;P@ssw0rd_${uuid}&quot;

// Guardar en variables de JMeter (visibles solo para este hilo)
vars.put(&quot;TEST_EMAIL&quot;, email)
vars.put(&quot;TEST_PASSWORD&quot;, password)

log.info(&quot;Thread ${tnum} -&gt; TEST_EMAIL=${email} TEST_PASSWORD=${password}&quot;)</stringProp>
            <stringProp name="scriptLanguage">groovy</stringProp>
          </JSR223Sampler>
